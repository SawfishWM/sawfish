# ../lisp/cycle.jl
msgid "Select the next window of the current workspace."
msgstr "Selecciona la siguiente ventana del escritorio actual."

# ../lisp/cycle.jl
msgid "Focus on the previous window of the current workspace."
msgstr "Selecciona la anterior ventana del escritorio actual."

# ../lisp/cycle.jl
msgid "Select the next window, cycling through all possible workspaces."
msgstr "Selecciona la siguiente ventana, entre todos los escritorios."

# ../lisp/cycle.jl
msgid "Select the previous window, cycling through all possible workspaces."
msgstr "Selecciona la ventana anterior, entre todos los escritorios."

# ../lisp/edge-flip.jl
msgid "Edge Flipping"
msgstr "Bordes activos/Cambio de bordes"

# ../lisp/edge-flip.jl
msgid "Flip to next viewport/workspace when pointer hits screen edge."
msgstr ""
"Ir al siguiente escritorio si el puntero está en el borde de la pantalla."

# ../lisp/edge-flip.jl
msgid "What hitting the screen edge actually flips."
msgstr "Lo que cambia al pulsar el borde de la pantalla."

# ../lisp/edge-flip.jl
msgid ""
"Number of milliseconds to wait after pointer hits screen edge before\n"
"flipping."
msgstr "Retardo en milisegundos para cambiar de escritorio."

