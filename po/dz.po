msgid "Merge with next"
msgstr "ཤུལ་མམ་དང་གཅིག་ཁར་མཉམ་བསྡོམས་འབད།"

#. ../lisp/sawfish/wm/workspace.jl
msgid "Merge with previous"
msgstr "ཧེ་མམ་གྱི་དང་གཅིག་ཁར་མཉམ་བསྡོམས་འབད།"

#. ../lisp/sawfish/wm/workspace.jl
msgid "Move workspace _right"
